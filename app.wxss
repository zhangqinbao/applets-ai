/**app.wxss**/
@import 'weui/weui.wxss';
.phcolor{
  color: #ACACAC;
}
.weui-btn_primary:not(.weui-btn_disabled):active {
  background-color: #1DBFB3;
}
.ai-no-net{
  text-align: center;
  padding-top: 114rpx;
  height: 100%;
  background: #fff;
}
.ai-no-net>view:first-child{
  width: 552rpx;
  height: 508rpx;
  margin: 0 auto;
  position: relative;
}
.ai-no-net>view:first-child image{
  width: 100%;
}
.ai-no-net>view:first-child view{
  position: absolute;
  color: #999;
  font-size: 24rpx;
  text-align: center;
  left: 0;
  bottom: 58rpx;
  width: 100%;
}
.ai-no-net button{
  width: 152rpx!important;
  border-radius: 56rpx;
  height: 56rpx;
  padding: 0;
  background: #1DBFB3;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
  line-height: 56rpx;
}